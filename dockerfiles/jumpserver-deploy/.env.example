# JumpServer Configuration
# Copy this file to .env and update the values

# Security Keys (Generate random strings for production)
SECRET_KEY=changeme_secret_key_must_be_long
BOOTSTRAP_TOKEN=changeme_bootstrap_token

# Database
DB_HOST=db
DB_PORT=5432
DB_USER=jumpserver
DB_PASSWORD=changeme_db_password
